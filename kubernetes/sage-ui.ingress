apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: sage-ui
  namespace: sage
  annotations:
    traefik.ingress.kubernetes.io/router.tls: ""
spec:
  rules:
  - host: localhost
    http:
      paths:
      - path: /
        pathType: Prefix
        backend:
          service:
            name: sage-ui
            port:
              number: 80